{
  "name": "minsk-veg-places",
  "version": "1.0.0",
  "description": "minsk-veg-places",
  "main": "",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"node-sass --include-path scss ./public/assets/styles/main.scss ./public/assets/styles/main.css\" \"nodemon ./server/bin/www\" ",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "bower install && typings install && tsc -p ./server",
    "build-css": "node-sass --include-path scss ./public/assets/styles/main.scss ./public/assets/styles/main.css",
    "start": "concurrently \"bower install\" \"npm run build-css\" \"npm run develop\""
  },
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "author": "Artsiom",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.13.2",
    "bower": "^1.7.9",
    "cookie-parser": "~1.3.5",
    "express": "^4.13.4",
    "serve-favicon": "~2.3.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/http": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/router": "^3.0.0-alpha.7",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.2",
    "angular2-in-memory-web-api": "0.0.12",
    "concurrently": "^2.0.0",
    "core-js": "^2.4.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "systemjs-builder": "^0.15.17",
    "yargs": "^4.7.1",
    "zone.js": "^0.6.12"
  }
}